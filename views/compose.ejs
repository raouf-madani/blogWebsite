<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/compose" method="POST" >
    <div class="form-group">
        <label>Title</label>
        <input name="pub" type="text" autocomplete="off" class="form-control"/>
    </div>
    <div class="form-group">
        <label>Post</label>
        <textarea name="pubContent" rows="5" cols="30" class="form-control"></textarea>
    </div>
    <button type="submit" name="compose" class="btn btn-primary">Publish</button>
</form>

<%- include("partials/footer") -%>